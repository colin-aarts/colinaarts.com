/*
** Garamond
*/

@font-face
	font-family: Garamond
	src: url(assets + '/fonts/garamond-italic.woff2') format('woff2'), url(assets + '/fonts/garamond-italic.woff') format('woff')
	font-weight: 500
	font-style: italic



/*
** Font Awesome
*/

$fa-font-path = '../../../node_modules/font-awesome-stylus/fonts'
@import '../../../node_modules/font-awesome-stylus/stylus/path'
@import '../../../node_modules/font-awesome-stylus/stylus/variables'
