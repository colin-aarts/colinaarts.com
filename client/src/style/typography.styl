@import '~style/config'

/*
** Delicious
*/

@font-face
	font-family: Delicious
	src: url(assets + '/fonts/delicious-roman.woff2') format('woff2'), url(assets + '/fonts/delicious-roman.woff') format('woff')
	font-weight: 400
	font-style: normal

@font-face
	font-family: Delicious
	src: url(assets + '/fonts/delicious-bold.woff2') format('woff2'), url(assets + '/fonts/delicious-bold.woff') format('woff')
	font-weight: 700
	font-style: normal

/*@font-face
	font-family: Delicious
